@charset "utf-8";

#footer .section:first-child { text-align: center; }

.figure-overlay .figure-overlay-container .figure-caption { text-align: center; }

.center-block { text-align: center; }

.transparent-header #masthead:not(.navbar-scrolled) .dropdown-menu li a { color: rgb(68, 68, 68); }

.dropdown-menu li a { color: rgb(68, 68, 68) !important; }

.menu .dropdown-menu > li > a { font-weight: bold !important; }

a { color: rgb(241, 94, 36); }

.menu .sidebar-widget i { color: rgb(255, 255, 255); }

h1.text-light, h2.text-light, h3.text-light, h4.text-light, h5.text-light, h6.text-light, .text-light h1, .text-light h2, .text-light h3, .text-light h4, .text-light h5, .text-light h6 { color: rgb(255, 255, 255) !important; }

#form-wysija-3 { display: inline-flex; }

.widget_wysija_cont .wysija-submit { display: block; margin-top: 29px; margin-bottom: 12px; }

.slide-menu .navbar-toggle { display: block; float: left; overflow: visible !important; }

.menu .navbar-brand { margin-top: 0px; margin-bottom: 0px; position: relative; text-shadow: none; transform: translate3d(0px, 0px, 0px); transition: height 0.3s ease 0s, width 0.3s ease 0s, line-height 0.3s ease 0s; white-space: nowrap; background-size: auto 100%; background-position: left center; background-repeat: no-repeat; backface-visibility: hidden; margin-right: 0px !important; }

.slide-on #masthead, .slide-on #navbar-slide, .slide-on .top-bar { transform: translateX(270px); }

.slide-on #content { transform: translateX(0px); }

#navbar-slide { position: fixed; top: 0px; right: 100%; z-index: 1110; box-sizing: border-box; box-shadow: rgba(0, 0, 0, 0.25) 5px 0px 5px -3px inset; height: 100% !important; }

.navbar-brand { float: right; padding: 13.5px 15px; font-size: 18px; line-height: 23px; height: 90px; }

#navbar-slide.menu { font-family: "Playfair Display", sans-serif; font-weight: 700; font-size: 16px; float: left; left: -270px; }

.navbar-toggle:hover .icon-bar { background: rgb(241, 94, 36) !important; }

#hamb_menu:hover { background: rgb(241, 94, 36); }

.header-logo-center-menu-below .logo-navbar, .header-logo-left-menu-below .logo-navbar, .header-logo-right-menu-below .logo-navbar, .header-logo-left-sidebar-right .logo-navbar { color: rgb(255, 255, 255); background: rgba(39, 55, 77, 0.91); }

.wysija-input { background-color: rgba(255, 255, 255, 0.87); color: rgb(60, 60, 60); border-color: rgba(241, 94, 36, 0) !important; }

.nav-special-btn { font-size: 20px; height: 60px; margin-top: 12px; font-weight: 600; display: inline-block; color: white !important; }

.left { text-align: left; }

.center { text-align: center; }

.right { text-align: right; }

.nav-special-btn:hover { color: rgb(241, 94, 36) !important; }

#navBookNow { background: rgba(241, 94, 36, 0); transition: all 0.1s ease-out 0s; border: 1px solid silver; padding: 12px 16px; text-transform: uppercase; }

@media (max-width: 767px) {
  #navBookNow { margin-top: 15px; height: 60px; border: 1px solid; font-size: 13px; }
}

#navBookNow:hover { border: 1px solid rgb(241, 94, 36); }

.go-top { background: rgb(241, 94, 36) !important; }

#navContact { margin-left: 10%; }

#navGallery { margin-left: 20%; }

input.wpcf7-form-control.wpcf7-submit { width: 100px; }

.txt-green { color: green; }
