@charset "utf-8";

html { box-sizing: border-box; }

*, ::before, ::after { box-sizing: inherit; }

body, div.container1 { font-family: "Miller Display Roman", georgia, "Times New Roman", times, serif; -webkit-font-smoothing: antialiased; font-size: 100%; }

body { background-color: rgb(74, 74, 74); display: block; margin: 0px; }

@font-face { src: url("/amharic/jiret.ttf"); }

p { font-family: "Miller Display Roman", georgia, "Times New Roman", times, serif; -webkit-font-smoothing: antialiased; font-size: 1em; line-height: 1.5em; }

h1 { font-size: 1.3em; }

h2 { font-size: 1.2em; }

h3, h4, h5 { font-size: 1.1em; }

td { padding: 2px 5px; }

div.container1 { max-width: 1400px; width: 100%; min-height: 98vh; display: block; padding: 0px 5px; margin: 0px auto; line-height: 1.5em; overflow: auto; }

div.header { display: block; width: 100%; height: 84px; background-image: url("/image/top-background-image1.jpg"); background-color: rgb(25, 40, 47); }

div.topnavigation { background-color: rgb(25, 40, 47); display: block; width: 100%; height: 46px; clear: both; z-index: 20; line-height: 1.5em; }

div.container2 { display: block; width: 100%; min-height: 70vh; margin: auto; padding: 10px; float: left; background-color: rgb(255, 255, 255); border: 0.4em solid rgb(209, 209, 165); }

div.sidebarleft { display: block; height: auto; width: auto; margin: 20px auto; float: left; overflow: hidden; }

div.leftsidenav { overflow: hidden; }

div.leftsidead { width: 100%; margin: 30px auto; text-align: center; }

.sidebarright { display: none; height: auto; margin: 20px auto; float: right; background-color: blue; }

.content { display: block; margin: 5px 10px; width: calc(100% - 235px); float: left; overflow: hidden; }

.content .searchblock { display: block; float: left; width: 100%; overflow: hidden; }

.searchblock #search-box { margin: 10px 20px; float: right; }

.searchblock #search-box input { padding: 5px; font-size: 1em; }

.searchblock #search-box input[type="text"] { width: 250px; }

.searchblock #search-box input[type="submit"] { cursor: pointer; }

.content .maincontent { display: block; float: left; width: 100%; height: 100%; }

.contentbottom { display: block; float: left; width: 100%; overflow: auto; margin: 15px auto; }

.contentbottom .bottomAd { display: block; width: 100%; float: left; overflow: hidden; }

.contentbottom .btmPromo { display: block; width: 100%; float: left; overflow: hidden; }

.container2bottom { display: block; float: left; width: 100%; clear: both; margin-left: auto; margin-right: auto; }

.container2bottom .btm2Ad { display: block; width: 100%; float: left; }

.container2bottom .btm2Promo { display: block; width: 100%; float: left; }

div.footernav { display: block; width: 100%; height: auto; float: left; line-height: 1.5em; background-color: rgb(169, 163, 163); clear: both; text-align: center; padding: 5px 10px; }

div.footernav { text-align: center; font-size: 1em; padding: 15px 10px; }

div.footernav a.foot { padding-left: 10px; padding-right: 10px; border-right: 1px solid rgb(255, 255, 255); line-height: 2em; text-decoration: none; }

div.footernav a.foot:last-child { border-right: none; }

div.footernav a.MyPlace::before { content: "Login / Join "; }

div.footernav a.foot:hover { background: rgb(255, 255, 0); }

div.footnote { display: block; width: 100%; height: auto; float: left; line-height: 1.7em; background-color: rgb(209, 209, 165); clear: both; text-align: center; padding: 5px 10px; margin-bottom: 15px; }

.appheader { position: fixed; top: 0px; left: 0px; z-index: 999; width: 100%; }

.headerLogin { color: rgb(17, 0, 252); font-weight: bold; width: auto; text-align: center; float: right; font-size: 0.8em; padding: 0.2em; position: relative; margin-top: 5px; z-index: 10; }

.headerLogin .log1 { padding: 4px; margin-right: 2em; }

.headerLogin .log1 a { text-decoration: none; color: rgb(255, 255, 255); }

.headerLogin .log1 a img.profilePic { width: 40px; height: 40px; border-radius: 100%; border: none; vertical-align: middle; }

.headerLogin .log1 a img.myplacePic { height: 16px; vertical-align: middle; }

a.sideMenuHeader img.myplacePic { width: 50px; height: 50px; }

.headerLogin .log1b { padding: 4px; }

.headerLogin .log1b a { text-decoration: none; color: rgb(255, 255, 255); }

.logoTime { width: auto; text-align: center; float: left; margin: 5px 15px; }

.logoTime #logo { vertical-align: middle; height: 50px; }

.logoTime #time { display: inline; margin-left: 50px; background-color: rgb(255, 255, 255); color: rgb(51, 85, 237); font-weight: bold; font-size: 0.9em; }

.log { margin-left: auto; margin-right: auto; height: 30px; padding-top: 8px; padding-bottom: 8px; }

.log a { text-decoration: none; padding: 5px; height: auto; background: rgb(31, 140, 255); border: 1px solid rgb(204, 204, 204); color: rgb(255, 255, 255); font-weight: bold; text-align: center; }

.log a:visited { color: rgb(255, 255, 255); text-decoration: none; }

.log a:hover { color: rgb(0, 0, 0); background: rgb(255, 255, 0); font-weight: bold; text-decoration: none; }

.log a:active { color: rgb(0, 0, 0); background: rgb(255, 255, 0); font-weight: bold; text-decoration: none; }

.logout { margin-left: auto; margin-right: auto; height: 30px; padding-top: 8px; padding-bottom: 8px; }

.logout a { text-decoration: none; padding: 5px; height: auto; background: rgb(5, 94, 23); border: 1px solid rgb(204, 204, 204); color: rgb(255, 255, 255); font-weight: bold; text-align: center; }

.logout a:visited { color: rgb(255, 255, 255); text-decoration: none; }

.logout a:hover { color: rgb(0, 0, 0); background: rgb(255, 255, 0); font-weight: bold; text-decoration: none; }

.logout a:active { color: rgb(0, 0, 0); background: rgb(255, 255, 0); font-weight: bold; text-decoration: none; }

.log a, .logout a { border-width: 4px; border-style: solid; border-color: rgb(223, 223, 223) rgb(102, 102, 102) rgb(51, 51, 51) rgb(133, 133, 133); padding: 0.25em 0.5em; }

@media only screen and (min-width: 1100px) {
  .sidebarright { display: none; }
  .content { display: block; margin: 5px 10px; width: calc(100% - 245px); float: left; }
}

@media only screen and (max-width: 1100px) and (min-width: 800px) {
  .content { width: calc(100% - 235px); margin: 5px 0px 5px 10px; }
}

@media only screen and (max-width: 800px) {
  div.sidebarleft { display: none; }
  .content { display: block; width: 100%; margin: 5px auto; float: none; }
  .searchblock #search-box { margin: 10px; }
  div.header { height: 60px; background-image: url("/image/760/top-background-image1.jpg"); }
  .headerLogin, .headerLogin .log1 a img.profilePic { margin-top: 3px; }
  .headerLogin .log1 a img.profilePic { width: 30px; height: 30px; border-radius: 100%; border: none; vertical-align: middle; }
  .logoTime { margin: 0px 10px; }
  .logoTime #logo { height: 40px; }
  .logoTime #time { margin-left: 10px; }
}

@media only screen and (max-width: 620px) {
  .logoTime #time, .headerLogin .log1b { display: none; }
  div.header { height: 50px; background-image: url("/image/400/top-background-image1.jpg"); }
  .newJoin { display: none; }
  div.container2 { padding: 0px; }
  div.content { margin: 5px 0px; }
}

@media only screen and (max-width: 400px) {
  .searchblock #search-box input[type="text"] { width: 200px; }
  .searchblock #search-box { margin: 5px; }
  div.container2 { padding: 0px; border: 0.1em solid rgb(209, 209, 165); }
  .content { display: block; margin: 2px; width: 100%; float: none; }
  div.header { height: 40px; background-image: url("/image/360/top-background-image1.jpg"); }
  .headerLogin, .headerLogin .log1 a img.profilePic { margin-top: 0px; }
  .headerLogin .log1 a img.profilePic { width: 30px; height: 30px; border-radius: 100%; border: none; vertical-align: middle; }
  .logoTime { margin: 0px 10px; }
  .logoTime #logo { height: 35px; }
  .logoTime #time { margin-left: 10px; }
  .newJoin { display: none; }
}

@media only screen and (max-width: 360px) {
  .headerLogin .log1 { display: none; }
}

.top-introHeader { width: 70%; margin: 5px auto; padding: 10px; clear: both; }

