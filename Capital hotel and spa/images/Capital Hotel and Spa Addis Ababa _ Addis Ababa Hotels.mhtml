Content-Type: text/css
Content-Transfer-Encoding: binary
Content-Location: https://www.ethiovisit.com/hotels/contactus/contactus-form-style.css

@charset "utf-8";

#cover.contactusCover { width: 100%; height: 100%; z-index: 105; top: 0px; left: 0px; display: none; position: fixed; background-color: rgba(50, 50, 72, 0.5); overflow: auto; }

#cover.contactusCover img#close { position: absolute; right: -14px; top: -14px; cursor: pointer; }

#cover.contactusCover div#popupContact { position: absolute; left: 40%; top: 17%; margin-left: -202px; margin-bottom: 50px; font-family: Arial, Verdana, sans-serif; }

#cover.contactusCover #formcontainer { max-width: 600px; min-width: 250px; padding: 10px 20px 10px 50px; border: 2px solid gray; border-radius: 10px; background-color: rgb(255, 255, 255); }

#cover.contactusCover .container { margin: 20px 0px; }

#cover.contactusCover h2 { background: rgb(0, 0, 102); color: rgb(255, 255, 255); padding: 20px 35px; margin: -10px -20px -10px -50px; text-align: center; border-radius: 10px 10px 0px 0px; }

#cover.contactusCover .disclaimer { background-color: rgb(223, 248, 255); padding: 10px 5px 10px 15px; font-size: 0.9em; font-family: "Times New Roman"; margin: -10px -20px -10px -50px; }

#cover.contactusCover .knowThisPlace { background-color: rgb(247, 247, 247); padding: 10px 5px 10px 15px; font-size: 0.9em; line-height: 1.2em; font-family: "Times New Roman"; margin: -10px -20px -10px -50px; }

#cover.contactusCover hr { margin: 10px -20px 50px -50px; border-width: 1px 0px 0px; border-right-style: initial; border-bottom-style: initial; border-left-style: initial; border-right-color: initial; border-bottom-color: initial; border-left-color: initial; border-image: initial; border-top-style: solid; border-top-color: rgb(204, 204, 204); }

#cover.contactusCover input[type="text"] { width: 82%; padding: 10px 10px 10px 40px; margin-top: 5px; border: 1px solid rgb(204, 204, 204); font-size: 16px; border-radius: 10px; }

#cover.contactusCover :focus { color: rgb(0, 0, 153); border: 1px solid rgb(153, 0, 0); background-color: rgb(255, 255, 153); }

#cover.contactusCover #scaptcha { width: 80px; height: 25px; padding: 5px 5px 5px 10px; border: 1px solid rgb(204, 204, 204); font-size: 16px; }

#cover.contactusCover .short_explanation { font-size: 0.8em; color: rgb(51, 51, 51); }

#cover.contactusCover #name { background-image: url("/contact/images/name1.png"); background-repeat: no-repeat; background-position: 5px 7px; }

#cover.contactusCover #email { background-image: url("/contact/images/email2.png"); background-repeat: no-repeat; background-position: 5px 7px; }

#cover.contactusCover select { padding: 10px; min-width: 50%; margin-left: 50px; font-size: 1.1em; border-radius: 10px; }

#cover.contactusCover textarea { background-image: url("/contact/images/msg2.png"); background-repeat: no-repeat; background-position: 5px 7px; width: 85%; height: 150px; padding: 10px 10px 10px 40px; resize: auto; margin-top: 5px; border: 1px solid rgb(204, 204, 204); font-size: 16px; border-radius: 10px; }

#cover.contactusCover #submit { width: 100%; text-align: center; display: block; background-color: rgb(255, 188, 0); color: rgb(255, 255, 255); border: 1px solid rgb(255, 203, 0); padding: 10px 0px; font-size: 3em; font-weight: bold; cursor: pointer; border-radius: 5px; margin-bottom: 40px; }

#cover.contactusCover label { display: block; width: 100%; font-weight: bold; }

#cover.contactusCover .scaptcha label { display: inline; padding-right: 10px; font-weight: bold; }

#cover.contactusCover span { color: rgb(255, 0, 0); display: block; width: 100%; }

#cover.contactusCover .error { color: rgb(255, 0, 0); font-size: 12px; margin-top: 5px; margin-bottom: 0px; }

#cover.contactusCover .inputTxtError { border: 1px solid rgb(255, 0, 0); color: rgb(14, 14, 14); }

#cover.contactusCover .short_explanation { font-family: Arial, sans-serif; font-size: 0.8em; color: rgb(51, 51, 51); }

#cover.contactusCover #submit_success_message { display: none; color: rgb(2, 14, 180); font-size: 1.5em; padding: 5px 10px 30px; text-align: center; line-height: 1.4em; }

#cover.contactusCover #successClose { width: 100%; text-align: center; display: block; background-color: rgb(26, 153, 6); color: rgb(255, 255, 255); border: 1px solid rgb(255, 203, 0); padding-top: 10px; font-size: 1.5em; font-weight: bold; cursor: pointer; border-radius: 5px; margin-top: 20px; }

#cover.contactusCover #phpMessage { display: none; color: rgb(255, 0, 0); width: 100%; font-size: 0.9em; padding: 5px; background-color: rgb(242, 242, 242); line-height: 1.5em; }

#cover.contactusCover .sendCopy { font-size: 0.8em; }

#cover.contactusCover #closeWindowDiv { text-align: center; display: none; width: 100%; margin: 20px auto; }

#cover.contactusCover #closeWindow { width: auto; text-align: center; background-color: rgb(160, 0, 0); color: rgb(255, 255, 255); padding: 5px 15px; font-size: 1em; cursor: pointer; border-radius: 5px; }

@media only screen and (max-width: 840px) {
  #cover.contactusCover #closeWindowDiv { display: block; }
}

@media only screen and (max-width: 400px) {
  #cover.contactusCover div#popupContact { margin-left: -160px; }
  #cover.contactusCover h2 { margin: -10px -20px -10px -10px; }
  #cover.contactusCover .disclaimer { margin: -10px -20px -10px -10px; }
  #cover.contactusCover .knowThisPlace { margin: -10px -20px -10px -10px; }
  #cover.contactusCover select { margin-left: 10px; }
}
