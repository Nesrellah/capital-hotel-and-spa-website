.listing-summary h4 { font-weight: normal; font-size: 1em; }

.listing-summary .heading { border-bottom: 1px solid rgb(229, 229, 229); padding-left: 5px; padding-top: 0px; padding-bottom: 0px; background-color: rgb(245, 245, 245); }

.listing-summary .footer { padding: 5px; }

.pagination { padding-top: 10px; padding-bottom: 5px; border-top: 1px solid rgb(195, 217, 224); border-bottom: 1px solid rgb(195, 217, 224); }

.result { padding-top: 5px; }
